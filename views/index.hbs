      {{!-- services section --}}
       <section id="services_section" class="cardbackground">
        {{!-- first row --}}
      <div class="container">
           <h1 class="text-center text-primary"> Courses </h1>
        <div class="row">

          {{!-- first services/ --}}

          {{#each services}}
          <div class="col-md-4">
            <div class="card" >
              <div class="card-body">
                <div class="container text-center">
                  <i class="{{this.icon}} services-icon"></i>
                </div>
                
                <h3>{{this.title}}</h3>
              <p>{{this.description}}</p>
              <a href="{{this.linktext}}" class="mr-3"></a>
              <a href="{{this.link}}">{{this.linktext}}</a>
